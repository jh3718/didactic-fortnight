Algoritmo "semnome"

Var
   peso:real
   altura:real
   imc:real
Inicio
      escreva("Escreva seu peso em kg=")
      leia(peso)
      
      escreva("Escreva sua altura em metros=")
      leia(altura)

              imc:=peso/altura

      escreval("O indice da massa corporal=",imc)

Fimalgoritmo